net: "./model_cifar100/model_cifar100_fine_tuning.prototxt"
type: "Adam"
test_iter: 100
test_interval: 1000
base_lr: 0.0005
display: 20
max_iter: 100000
lr_policy: "step"
gamma: 0.2
momentum: 0.9
weight_decay: 0.0005
stepsize: 750
snapshot: 10000
snapshot_prefix: "./caffenet_train_snapshot_fine_tuning"
solver_mode: GPU
